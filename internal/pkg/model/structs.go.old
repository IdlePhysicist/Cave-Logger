package model

import "time"

type Cave struct {
  Name    string
  Region  string
  Country string
  SRT     bool
  Visits  int
}

type Caves [int]Cave

type Caver struct { // is it better to do f & l or just name ?
  First string
  Last  string
  Club  string
}

type Cavers [int]Caver

type Log struct {
  Date  *time.Time
  Cave  string
  Names *Cavers
  Notes string
}

type Logs []Log
